module check

imports

	include/TypeSystemLanguage

rules // Extensions for error/warning/note reporting
	
	constraint-error:
		_ -> <fail>
 
	constraint-warning:
		_ -> <fail>
		
	constraint-note:
		_ -> <fail>

rules

  nabl-check-disable-unresolved = string-starts-with(|"trans/") + string-starts-with(|"analysis/") + string-starts-with(|"src-gen/")
